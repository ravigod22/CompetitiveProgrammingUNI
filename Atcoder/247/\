#include <bits/stdc++.h>
using namespace std;

int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);
    int tt = 1;
    while (tt--) {
        string s;
        cin >> s;
        string tmp = s.substr(1);
        cout << tmp << "\n";
        tmp = "0" + tmp;
        cout << tmp << "\n";
    }
    return 0;
}
